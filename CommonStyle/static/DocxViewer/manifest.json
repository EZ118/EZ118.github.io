{
  "name": "Word PWA",
  "short_name": "Word",
  "descriptions": "View Docx File",
  "start_url": "./",
  "background_color": "#323639",
  "display": "minimal-ui",
  "theme_color": "#185ABD",
  "icons": [
    {
      "src": "./icon.png",
      "type": "image/png"
    }
  ],
  "display_override": ["window-controls-overlay"],
  
  "file_handlers": [
    {
      "action": "./",
      "accept": {
        "text/*": [
          ".docx"
        ]
      }
    }
  ]
}
